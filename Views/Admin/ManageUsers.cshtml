@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>

@{
    ViewData["Title"] = "Kullanıcı Yönetimi";
}

<h1>Kullanıcı Yönetimi</h1>

<table class="table">
    <thead>
        <tr>
            <th>Kullanıcı Adı</th>
            <th>E-Posta</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>
